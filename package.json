{
  "name": "autolabsuite-docs",
  "description": "AutoLabSuite — платформа автоматизации проверки лабораторных и оценки по рубрикам",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "lint:md": "remark . --ext .md,.mdx --ignore-path .remarkignore -u remark-gfm -u remark-preset-lint-recommended",
    "lint:md:fix": "remark . --ext .md,.mdx --ignore-path .remarkignore -u remark-gfm -u remark-preset-lint-recommended --output",
    "lint:openapi": "redocly lint docs/api/openapi.yaml"
  },
  "devDependencies": {
    "remark": "^15.0.1",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "@redocly/cli": "^1.29.0"
  }
}
